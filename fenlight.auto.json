{
    "name": "FenLight - Auto Play",
    "plugin": "plugin.video.fenlight",
    "priority": 500,     
    "is_resolvable" : "true", 
    "play_movie": "executebuiltin://RunPlugin(plugin://plugin.video.fenlight/?mode=playback.media&media_type=movie&tmdb_id={tmdb}&autoplay=true)",
    "play_episode": "executebuiltin://RunPlugin(plugin://plugin.video.fenlight/?mode=playback.media&media_type=episode&tmdb_id={tmdb}&season={season}&episode={episode}&autoplay=true)"
}